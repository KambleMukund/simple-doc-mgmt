{
  "name": "simple-doc-mgmt",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://kamblemukund.github.io/simple-doc-mgmt",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "ag-grid-community": "^25.2.1",
    "ag-grid-react": "^25.2.0",
    "axios": "^0.21.1",
    "date-fns": "^2.22.1",
    "json-server": "^0.16.3",
    "multer": "^1.4.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-modal": "^3.13.1",
    "react-query": "^2.23.1",
    "react-query-devtools": "^2.6.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix ./src/**/*.js",
    "start:server": "cd src\\server && node mock-server.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=true",
    "cypress:run:e2e": "cypress run --config video=true,integrationFolder=\"cypress/e2e\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^7.5.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "prettier-eslint": "^12.0.0",
    "redux-devtools": "^3.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js|html|css}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js}": [
      "npm run lint:fix",
      "npm run lint"
    ]
  }
}
